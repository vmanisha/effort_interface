[x][oName]; } } for( var x = 0; x < oDoc.anchors.length; x++ ) { if( oDoc.anchors[x].name == oName ) { return oDoc.anchors[x]; } } for( var x = 0; document.layers && x < oDoc.layers.length; x++ ) { var theOb = MWJ_findObj( oName, null, oDoc.layers[x].document ); if( theOb ) { return theOb; } } if( !oFrame && window[oName] ) { return window[oName]; } if( oFrame && oFrame[oName] ) { return oFrame[oName]; } for( var x = 0; oFrame && oFrame.frames && x < oFrame.frames.length; x++ ) { var theOb = MWJ_findObj( oName, oFrame.frames[x], oFrame.frames[x].document ); if( theOb ) { return theOb; } } return null;}var xx,yyfunction getCoords(n) { var tt; tt = MWJ_findObj(n); if (bw.ns4) { xx = tt.x; yy = tt.y; } else { xx = 0; yy = 0; while (tt != null) { xx += tt.offsetLeft; yy += tt.offsetTop; tt = tt.offsetParent; } } xx -= 6; yy += 12;}</script> <table cellspacing="0" cellpadding="0" width="90%" align="center" valign="top" bgcolor="#346565" border="0" summary="This table contains the layout for the entire page."> <!-- outermost table --> <tbody> <tr> <td> <table cellspacing="0" cellpadding="0" bgcolor="#346565" border="0" width="100%" height="110"> <tbody> <tr class="logobkgrd"> <td valign="middle" bgcolor="#346565" background="/themes/wznew/logo_banner_med_top.gif"> <div class="hzmenu"> <a class="menuitem" href="/content.php/system/index.htm">Home</a> | <a name="hzwznew_3" class="menuitem" href="" onmouseover="getCoords('hzwznew_3');oM.m['hzwznew_3'].b.moveIt(xx,yy);oM.showsub('hzwznew_3');return false" onmouseout="oM.mout();">The University</a> | <a name="hzwznew_4" class="menuitem" href="/content.php/home/audiences.htm" onmouseover="getCoords('hzwznew_4');oM.m['hzwznew_4'].b.moveIt(xx,yy);oM.showsub('hzwznew_4');return false" onmouseout="oM.mout();">Especially For...</a> | <a name="hzwznew_5" class="menuitem" href="/content.php/university/contact.html" onmouseover="getCoords('hzwznew_5');oM.m['hzwznew_5'].b.moveIt(xx,yy);oM.showsub('hzwznew_5');return false" onmouseout="o