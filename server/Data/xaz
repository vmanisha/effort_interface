primeArea" style="width:552px;"> <div style="display: none"> Generated from local file. Cache size:400 (not visible in beta/prod) </div> <div class="blockHeader"> <h1 class="topic">African American Music</h1> </div> <!-- No encyclopedia art --> <div id="textadslotdocwall" class="wallpagetextad"> <div class="ad google textadgoogle"> <script type="text/javascript" language="javascript"><!-- <!-- function google_ad_request_done(google_ads) { var s = ''; /* Verify that there are actually ads to display. */ if (google_ads.length <= 0) { s += '\r\n<!-- No google ads returned. -->'; document.write(s); return; } else { /* There are ads */ var oneline = "True" == "False"; var surround = "True" == "False"; var surrdiv = ""; var twocolumn = "True" == "False"; var i; /* two columns */ if(surround) { s+='<div'; if(surrdiv.length > 0) { s+=' class=\"' + surrdiv + '\"'; } s+='>'; } var oneline = "True" == "False"; s += '<h5><a href=\"' + google_info.feedback_url + '\" target=\"_blank\">Ads by Google</a></h5>'; s+= '<div class=\"qandastyletextad\">'; if (google_ads.length >= 1) { var specdnumber = 3; //alert(specdnumber); if(0 >= specdnumber) { specdnumber = 150; } var colSplit = -1; if(twocolumn){ colSplit = specdnumber / 2; s += '<div class="adcol1">' } for(i=0; i < google_ads.length && i < specdnumber; ++i) { if(twocolumn && i==colSplit){ s += '</div>' s += '<div class="adcol2">' } if(oneline) { s += '<p>' + '<a href="' + google_ads[i].url + '" target="_top" ><span class="title">' + google_ads[i].line1 + '</span></a>' + '<span class="desc">' + google_ads[i].line2 + ' ' + google_ads[i].line3 + '</span> ' + '<a href="' + google_ads[i].url + '" target="_top" ><span class="url">' + google_ads[i].visible_url + '</span></a>' + '</p>'; } else { s += '<p>' + '<a href="' + google_ads[i].url + '" target="_top" >' + '<span class="title">' + google_ads[i].line1 + '</span></a><br />' + '<span class="desc noldesc">' + google_ads[i].line2 + ' ' + google_ads[i].line3 + '</span><br />' + '<a hre