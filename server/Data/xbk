refix+'/.element/swf/2.0/breaking_news/bn_dev.swf';break; case 'Developing Story':leftColor='cnnBlack';rightColor='cnnYellow';break; case 'Watch Now':leftColor='cnnBlue';rightColor='cnnBlue';break; case 'Connect with CNN':leftColor='cnnBlue';rightColor='cnnBlack';flashURL=cdn_prefix+'/.element/swf/2.0/breaking_news/bn_connectWithCNN.swf';break; case 'Live Now (sponsored)':leftColor='cnnBlue';rightColor='cnnBlack';flashURL=cdn_prefix+'/.element/swf/2.0/breaking_news/bn_liveNow.swf';break; case 'Live Now':leftColor='cnnBlue';rightColor='cnnBlack';flashURL=cdn_prefix+'/.element/swf/2.0/breaking_news/bn_liveNow.swf';break; default:return ''; } //new Effect.Appear('cnn_bnbrgt2',{duration:2}); return cnnRenderGenericBanner(object,flashURL,leftColor,rightColor);}</script> <div id="cnnBannerContainer"></div> <script type="text/javascript">CSIManager.getInstance().call('/.element/ssi/www/breaking_news/3.0/banner.html','','cnnBannerContainer',cnnRenderDomesticBanner);</script> <div id="cnnSetEditionContainer"></div> <div id="cnnMakeHPContainer"></div> <div id="cnnSubNav"> <a id="cnnSubNavleft" title="HEALTH MAIN" href="/HEALTH">HEALTH MAIN</a> <a title="LIVING WELL" href="/HEALTH/livingwell">LIVING WELL</a> <a title="DIET &amp; FITNESS" href="/HEALTH/diet.fitness">DIET &amp; FITNESS</a> <a title="MENTAL HEALTH" href="/HEALTH/mentalhealth">MENTAL HEALTH</a> <a title="CONDITIONS" href="/HEALTH/conditions">CONDITIONS</a> <a id="cnnSubNavright" class="cnnSubNavCurrent" title="HEALTH A-Z" href="/HEALTH/health.a.z">HEALTH A-Z</a> </div> <!-- OVERLAY MENU --> <!--[if lte IE 7]><style type="text/css"> #cnnHealthQuestion .clear {float:left;}</style><![endif]--> <div class="cnnOverlayMenuContainer" style="left:6px; top:6px;"> <div id="cnnHealthQuestion" class="cnnOverlayMenu"> <!-- Define the borders and content for the overlay menu here. --> <div class="cnnShareThisBox" style="width:609px;"> <div class="cnnShareBoxHeader"> <div class="cnnShareBoxHeaderTL"></div> <div class="cnnShareBo