m($('#fields_email').val()) == '') $('#fields_email').val('Email Address'); icpForm1711["fields_email"].focus(); return false; } return true; } function watermark(inputId, text) { var inputBox = document.getElementById(inputId); if (inputBox.value.length > 0){ if (inputBox.value == text) inputBox.value = ''; inputBox.style.color = 'black'; } else { inputBox.value = text; inputBox.style.color = 'lightgrey'; }}function checkCookie(){ var username=$.cookie("lpp_hotchalk"); if (username=="news_letter"){ icpForm1711.redirect_url_icp.value='http://www.lessonplanspage.com'; $.cookie("lpp_hotchalk", null); icpForm1711.submit(); }}</script> <!-- End Monthly Newsletters --> <!-- Begin Box Left Ads Widget Area --> </div> <!-- #left-widget-area --> <!-- Begin Box Left Ads Widget Area --> <div class="textwidget"></div> <!-- End Box Left Ads Widget Area --> </div> </div> </div> <div id="main"> <div id="mainnav" class="clearfix"> <div class="menu-mainmenu-container"> <ul id="nav" class="menu"> <li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-6836"><a href="#">Classroom</a> <ul class="nav1"> <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-4697"><a href="http://www.lessonplanspage.com/submit-a-lesson-plan/">Create a Lesson Plan</a></li> <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-4685"><a href="http://www.lessonplanspage.com/mra-htm/">Most Recent Additions</a></li> <li id="havechild" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-6840"><a title="By Subject">By Subject</a> <ul class="nav2"> <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-4669"><a href="http://www.lessonplanspage.com/art-htm/">Art</a></li> <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-4675"><a href="http://www.lessonplanspage.com/ci-htm/">Computers &amp; Internet</a></li> <li class="menu-item menu-item-type-post_type menu-item-object-page me