achedProduct.set(product_name, id); warningTip(id, parent_div_id); } } function removeAll(){ var len = CapterraCompare.products.length; for(i=0;i<len;++i){ var id = CapterraCompare.products.pop(); toggleCompareButtons(id); $j('div.'+id).each(function(){ $j(this).removeClass('show'); }); $j('li.compare_list_'+id).remove(); } CapterraCompare.product_names.length = 0; $j('div#compare_tooltip_body > h3').replaceWith('<h3>Products to Compare (0)</h3>'); $j.cookie('capterra_compare', { short_name: 'emr', pid_list: CapterraCompare.products, pname_list: CapterraCompare.product_names }); } function removeProduct(id){ toggleCompareButtons(id); $j('div.'+id).each(function(){ $j(this).removeClass('show'); }); var index = $j.inArray(id, CapterraCompare.products); CapterraCompare.products.splice(index,1); CapterraCompare.product_names.splice(index,1); $j('li.compare_list_'+id).slideUp(300, function(){ $j(this).remove();}); $j('div#compare_tooltip_body > h3').replaceWith('<h3>Products to Compare ('+CapterraCompare.products.length+')</h3>'); $j('div#replace_tooltip_body > h3').replaceWith('<h3>Replace a Product ('+CapterraCompare.products.length+')</h3>'); // update url & cookie $j.cookie('capterra_compare', { short_name: 'emr', pid_list: CapterraCompare.products, pname_list: CapterraCompare.product_names }); } $j(window).scroll(function() { $j('div#compare_tooltip').hide(); }); function warningTip(id, parent_div_id){ if (!(parent_div_id === 'undefined' || parent_div_id === null || parent_div_id === '')) { var button = $j('div#' + parent_div_id + ' a.' + id + '.add'); } else { var button = $j('a.' + id + '.add'); } button.tooltip({offset:[5,95], effect:'slide', tip:'#replace_tooltip'}); button.data('tooltip').show(); } function loadCompares(){ // Load saved compares from JSON cookie if it exists and we're in a directory that's saved // TODO: error processing var cookie = $j.cookie('capterra_compare'); if(cookie !== null && cookie.short_name === 'emr'){ var plist = cookie.pid_list.toString(), pnames = cookie.pname_list.toString(); if(plist !== "[]"){ var pid_list = plist.substring(0,plist.length).replace(/ /g,'').split(","), pname_list = pnames.substring(0,pnames.length).replace(/\"/g,'').split(","); $j.each(pid_list, function(idx, val) { var prod_name = pname_list[idx]; if (!(prod_name === 'undefined' || prod_name === null || prod_name === '')){ addProduct(parseInt(val,10), prod_name, null); } }); } } else if (cookie === null){ $j.cookie('capterra_compare', { short_name: 'emr', pid_list: [], pname_list: [] }); } // if cookie exists but we're in a different directory, do nothing } function clearCompares(){ removeAll(); $j.cookie('capterra_compare', { short_name: 'emr', pid_list: [], pname_list: [] }); } /* ]]> */ </script> <script type="text/javascript"> var _gaq = _gaq || []; _gaq.push( ['_setAccount', 'UA-126190-1'],['_trackPageview'],['_trackPageLoadTime'] ); (function() { var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true; ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s); })();</script> </head> <body> <div id="header"> <div id="header_content"> <div id="logotype"> <a href="/"><img src="http://cdn0.capterra-static.com/newlayout/images/2012/capterra_logo.png" /></a> </div> <div id="header_right"> <div id="nav"> <ul class="nav"> <li class="home"><a href="/" title="Home">HOME</a></li> <li class="buyer"><a href="/software_buyers" title="buyer">BUYERS</a></li> <li class="about-us"><a href="/about_capterra" title="About">ABOUT US</a></li> <li class="login"><a href="/vp" title="Login">LOGIN</a></li> </ul> <div class="reset"></div> </div> <!-- nav div --> <div id="search-top"> <form action="/search" method="post"> <div style="margin:0;padding:0"> <input name="authenticity_token" type="hidden" value="397d11a18a0b1dbc2f48fecc3c8070c0f63d42b7" /> </div> <input id="sea