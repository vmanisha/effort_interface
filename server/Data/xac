/images/t4a/search_basic_search_button.gif" onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;dnn$ctr6181$GenericBasicSearch$ibSearch&quot;, &quot;&quot;, true, &quot;BasicSearch&quot;, &quot;&quot;, false, false))" style="border-width:0px;" /> </div> </div> <br clear="all" /> <script type="text/javascript"> function validateNameCategory(source, args){ var businessName = document.getElementById('dnn_ctr6181_GenericBasicSearch_txtBusinessName'); var category = document.getElementById('dnn_ctr6181_GenericBasicSearch_ddlCategory'); var category = document.getElementById('dnn_ctr6181_GenericBasicSearch_ddlCategory'); } function validateState(source, args){ var businessName = document.getElementById('dnn_ctr6181_GenericBasicSearch_txtBusinessName'); var category = document.getElementById('dnn_ctr6181_GenericBasicSearch_ddlCategory'); var state = document.getElementById('dnn_ctr6181_GenericBasicSearch_ddlState'); if(businessName.value.length > 0){ args.IsValid = true; }else if(businessName.value.length == 0 && category.selectedIndex == 0 && state.selectedIndex == 0){ alert("Please enter a business name or select a category and location."); args.IsValid = false; }else if(businessName.value.length == 0 && category.selectedIndex == 0 && state.selectedIndex > 0){ alert("Please enter a business name or select a category."); args.IsValid = false; } else if(businessName.value.length > 0 || category.selectedIndex > 0 && state.selectedIndex == 0){ alert("Please select a state."); args.IsValid = false; } }</script> <!-- End_Module_6181 --> </div> </div> </div><a name="4975"></a><span id="dnn_ctr4975_ContentPane" class="DNNAlignleft"> <!-- Start_Module_4975 --> <div id="dnn_ctr4975_ModuleContent"> <script type="text/javascript">//preload imagevar collimg = new Image();collimg.src = "/images/t4a/collapse.gif";var expimg = new Image();collimg.src = "/images/t4a/expand.gif";function ShowHideLayer(boxID) { /* Obtain reference for the selected boxID laye