", attribs); if (newWind.opener == null) { window.opener = self; }}function goChannel() { var me = document.channels.Conditions.options[document.channels.Conditions.selectedIndex].value; if (me != "Channels") { location = "http://www.docguide.com/news/content.nsf/channel?OpenForm&id=48DDE4A73E09A969852568880078C249&c=" + me; }}function search(me) { var query = escape(document.frmSearch.search.value); if (query == ""){ alert("Please enter something in the search box."); } else if (document.frmSearch.searchbox[0].checked == true) location = "http://www.docguide.com/news/content.nsf/$$SearchNews?SearchView&Query=" + query + "&SearchOrder=2&SearchMax=50&id=48DDE4A73E09A969852568880078C249"; else if (document.frmSearch.searchbox[1].checked == true) searchBookstore(query); else if (document.frmSearch.searchbox[4].checked == true) searchMA(query); else if (document.frmSearch.searchbox[2].checked == true) location = "http://www.ncbi.nlm.nih.gov/htbin-post/Entrez/query?db=m&form=4&Term=" + query + "&dopt=r&dispmax=50"; else if (document.frmSearch.searchbox[3].checked == true) searchWeb(query); else if (document.frmSearch.searchbox[5].checked == true) location = "http://www.docguide.com/news/content.nsf/$$SearchAll?SearchView&Query=" + query + "&SearchOrder=2&SearchMax=50&id=48DDE4A73E09A969852568880078C249"; else{ alert("You must select a search location first."); }}function searchMA(me) { var redir = ""; confArray = MakeArray(me, "%20"); for (var q = 0 ; q < confArray.length ; q++) { if ( q == confArray.length - 1) redir = redir + "[DocBody]CONTAINS(" + confArray[q] + ")"; else redir = redir + "[DocBody]CONTAINS(" + confArray[q] + ")OR"; } location = "http://www.docguide.com/dg.nsf/conferences/?searchview&query=" + redir + "&SearchOrder=1&SearchMax=50&m=any";}function searchBookstore(me) { document.frmSearch.title.value = me; document.frmSearch.action = "http://www.lb.com/cgi-bin/searchlb?/Z105231@@7E5E22283616102A046058302E000C4D"; document.frmSearch.method = "post"; document.frmSearch.submit();}function searchWeb(me) { var sengine = document.frmSearch.Engine.value; var urlb = ""; var urlf = ""; if (sengine == "Infoseek") { urlf = "http://infoseek.go.com/Titles?qt="; urlb = "&col=WW&sv=IS&lk=noframes&svx=home_searchbox"; } else if (sengine == "AltaVista") urlf = "http://www.altavista.com/cgi-bin/query?pg=q&kl=XX&q="; else if (sengine == "Excite") urlf = "http://search.excite.com/search.gw?search="; else if (sengine == "HotBot") { urlf = "http://www.hotbot.com/?MT="; urlb = "&SM=MC&DV=0&LG=any&DC=10&DE=2&_v=2&OPs=MDRTP"; } else if (sengine == "Lycos") urlf = "http://www.lycos.com/cgi-bin/pursuit?matchmode=and&cat=lycos&query="; else if (sengine == "Yahoo") urlf = "http://search.yahoo.com/bin/search?p="; else if (sengine == "MetaCrawler") { urlf = "http://search.go2net.com/crawler?general="; urlb = "&method=0&target=&region=0&rpp=20&timeout=5&hpe=10"; } else if (sengine =="Ask Jeeves") { urlf = "http://www.askjeeves.com/AskJeeves.asp?ask="; urlb = "&qSource=0&origin=0&site_name=Jeeves&metasearch=yes"; } location = urlf + me + urlb;}function GlobalEdition(){ if (getCookie('Newsletter') == false) { var expdate = new Date (); expdate.setTime (expdate.getTime() + (1000 * 60 * 60 * 24 * 30)); setCookie ("Newsletter", "y",expdate); openWindow('http://www.docguide.com/gpc.nsf/doc?CreateDocument&n=1746&id=48DDE4A73E09A969852568880078C249&t='+expdate.getTime()+'&l=cardionews&u=http://www.docguide.com/Global-Editon-Interstitial.html','width=450,height=230,menubar=no,resizable=yes,scrollbars=no,status=no,toolbar=no') }}</script></p> <table border="0" width="775" cellspacing="0" cellpadding="0" align="top"> <tbody> <tr> <td rowspan="3" bgcolor="#000000"> <a style="color:white" href="http://www.pslgroup.com/docguide.htm"><img src="/gpimgs/main/gelogo.gif" width="179" height="93" border="0" /></a> </td> <td bgcolor="#152c66" width="591" colspan="2"> <center> <font face="Arial" size="5" color="#5e96f9"> Unregistered User</font> <br /> <a style="color:#ffffff" href="javascript:openWindow('http://www.docguide.com/dgc.nsf/RegisterGlobalEdition?OpenForm&amp;code=GlobalEditionPerPSL','width=500,height=470,menubar=no,resizable=yes,scrollbars=yes,status=no,toolbar=no,location=no')"> <font face="Arial" size="1" color="#ffffff"> If this is not your name, click here. </font> </a> </center> </td> <td bgcolor="#152c66" valign="top"> <a href="javascript:openWindow('http://www.docguide.com/dgc.nsf/RegisterGlobalEdition?OpenForm&amp;code=GlobalEditionPerPSL&amp;geptext=personalise','width=500,height=470,menubar=no,resizable=yes,scrollbars=yes,status=no,toolbar=no,location=no')"> <img src="/gpimgs/main/personalise.gif" width="132" height="20" border="0" align="right" /> </a> </td> </tr> <tr> <td bgcolor="#5e96f9" align="left" width="10" valign="top" height="25"> <img src="/gpimgs/main/rightcoin.gif" width="10" height="25" /> </td> <td bgcolor="#7796ed" width="590" align="center"> <font color="white" size="2" face="Arial"><b> <a style="color:white" href="http://www.docguide.com/dgc.nsf/contact/webmaster.htm?OpenDocument&amp;id=48DDE4A73E09A969852568880078C249">Contact Us</a> | <a style="color:white" href="javascript:openWindow('http://www.docguide.com/dgc.nsf/RegisterGlobalEdition?OpenForm&amp;code=GlobalEditionPerPSL','width=500,height=470,menubar=no,resizable=yes,scrollbars=yes,status=no,toolbar=no,location=no')"> Order Now</a> | <a style="color:white" href="http://www.docguide.com/dgc.nsf/web-journals?Openform&amp;id=48DDE4A73E09A969852568880078C249">Journals</a> | <a style="color:white" href="http://www.lb.com/cgi-bin/cgiwrap/bookshelf?/Z105231~id~">Bookstore</a> | <a style="color:white" href="http://www.docguide.com/dgc.nsf/RegisterAColleague?OpenForm&amp;id=48DDE4A73E09A969852568880078C249">Register a colleague</a> </b> </font> </td> <td valign="top" bgcolor="#7796ed" align="right" width="139"> <img src="/gpimgs/main/leftcoin.gif" width="134" height="25" /> </td> </tr> <!-- == End Top Bar == --> <!-- === Channel Selection == --> <tr> <form name="channels"></form> <td align="right" bgcolor="#3366cc" valign="bottom" colspan="3"><select name="Conditions" onchange="goChannel()"><option value="Channels">Select a Channel</option><option value="Acne">Acne</option><option value="AIDS%20and%20HIV">AIDS and HIV</option><option value="Allergy%20Other">Allergy Other</option><option value="Alzheimer%27s">Alzheimer's</option><option value="Anaesthesiology%20Other">Anaesthesiology Other</option><option value="Angina%20Pectoris%2fMI">Angina Pectoris/MI</option><option value="Anxiety">Anxiety</option><option value="Arthritis%20Other">Arthritis Other</option><option value="Asthma">Asthma</option><option value="Back%20Pain">Back Pain</option><option value="Bacterial%20Infections">Bacterial Infections</option><option value="Bladder%20Cancer">Bladder Cancer</option><option value="Bone%20Marrow%20Transplantation">Bone Marrow Transplantation</option><option value="Breast%20Cancer">Breast Cancer</option><option value="Cardiology%20Other">Cardiology Other</option><option value="Cataract">Cataract</option><option value="Cell%20Transplantation">Cell Transplantation</option><option value="Cervical%20Cancer">Cervical Cancer</option><option value="Cholesterol%2fLipid%20disorders">Cholesterol/Lipid disorders</option><option value="Cirrhosis">Cirrhosis</option><option value="Clinical%20Pharmacology">Clinical Pharmacology</option><option value="Colorectal%20Cancer">Colorectal Cancer</option><option value="Congestive%20Heart%20Failure">Congestive Heart Failure</option><option value="Contact%20Dermatitis">Contact Dermatitis</option><option value="Contraception">Contraception</option><option value="COPD">COPD</option><option value="Cystic%20Fibrosis">Cystic Fibrosis</option><option value="Dental%20and%20Oral%20Disorders">Dental and Oral Disorders</option><option value="Depression">Depression</option><option value="Dermatology%20Other">Dermatology Other</option><option value="Diabetes">Diabetes</option><option value="Dialysis">Dialysis</option><option value="Eating%20Disorders">Eating Disorders</option><option value="Elbow">Elbow</option><option value="Emergency%20Medicine">Emergency Medicine</option><option value="Endocrinology%20Other">Endocrinology Other</option><option value="Epilepsy">Epilepsy</option><option value="Erectile%20Dysfunction">Erectile Dysfunction</option><option value="Fibromyalgia">Fibromyalgia</option><option value="Gastro%20Other">Gastro Other</option><option value="Genetics">Genetics</option><option value="Genitourinary%20Other">Genitourinary Other</option><option value="Geriatrics">Geriatrics</option><option value="GERD%2fGastritis">GERD/Gastritis</option><option value="Glaucoma">Glaucoma</option><option value="H%2e%20Pylori%2fUlcer">H. Pylori/Ulcer</option><option value="Haematology%20Other">Haematology Other</option><option value="Hair%20Loss">Hair Loss</option><option value="Headache">Headaches</option><option value="Hepa%2fBiliary%20Other">Hepa/Biliary Other</option><option value="Hepatitis">Hepatitis</option><option value="Hip">Hip</option><option value="HRT">HRT</option><option value="Hypertension">Hypertension</option><option value="IBD">IBD</option><option value="Immunology">Immunology</option><option value="Infectious%20Other">Infectious Other</option><option value="Infertility">Infertility</option><option value="Intensive%20Care">Intensive Care</option><option value="Irritable%20Bowel%20Syndrome">Irritable Bowel Syndrome</option><option value="Knee">Knee</option><option value="Leukemias">Leukemias</option><option value="Liver%20Cancer">Liver Cancer</option><option value="Lung%20Cancer">Lung Cancer</option><option value="Lupus">Lupus</option><option value="Lymphomas">Lymphomas</option><option value="Melanoma">Melanoma</option><option value="Menopause">Menopause</option><option value="Multiple%20Sclerosis">Multiple Sclerosis </option><option value="Nephrology%20Other">Nephrology Other</option><option value="Neurologic%20Other">Neurologic Other</opt