 ""); } while (temp.indexOf("'") != -1) { temp = temp.replace(/'/, ""); } while (temp.indexOf('&') != -1) { temp = temp.replace(/&/, "and"); } while (temp.indexOf(',') != -1) { temp = temp.replace(/\,/, ""); } while (temp.indexOf('(') != -1) { temp = temp.replace(/\(/, ""); } while (temp.indexOf(')') != -1) { temp = temp.replace(/\)/, ""); } return temp; } function submitForm() { var url = "http://www.worldwidelearn.com/campus-location/"; var deg = string_replace(document.searchbox.degree.value); if (deg == "all") { deg = "degree-all"; } var subj = string_replace(document.searchbox.subject.value); if (subj == "all") { subj = "subject-all"; } var prog = string_replace(document.searchbox.program.value); if (prog=="" || prog=="all" || prog=="0") { var prog = "program-all"; } var concen = string_replace(document.searchbox.concentration.value); if (concen=="" || concen=="all" || concen=="0") { concen = "concentration-all"; } url += "online/city-all/" + deg + "/" + subj + "/" + prog + "/" + concen + ".php"; //orginial code window.document.location = url; }</script> <div id="sbc"> <script language="JavaScript"> var parent_1 = new Array("Fashion","Film & Television","Fine Art & Craft","Graphic Design","Interior Design","Multimedia & Web Design","Music","Photography","Theater Arts"); var parent_2 = new Array("Accounting","Business (General)","Business Administration","Customer Service","E-Commerce","Entrepreneurship","Finance","Human Resources","Insurance & Risk Management","International Business","Management & Leadership","Marketing & Advertising","Office Administration","Operations Management","Organizational Psychology","Project Management","Public Relations","Real Estate","Retail & Sales","Sports Management"); var parent_3 = new Array("Adult Education","Assessment & Evaluation","Child Development","Corporate Training","Curriculum Design","Distance Education","Early Childhood Education","Education (General)","Educational Administration","Educational Technology","ESL","Higher Education","K-12 Education","Literacy & Reading","Physical & Health Education","Special Education","Teacher Aide","Teaching License"); var parent_4 = new Array("Architecture & Landscape Design","Civil Engineering","Computer & Software Engineering","Construction","Electrical Engineering","Engineering (General)","Engineering Management","Environmental Engineering","Industrial Engineering","Material Engineering","Mechanical Engineering","Quality Control & Safety","Systems Engineering"); var parent_5 = new Array("Alternative Medicine","EMS & Paramedic","Gerontology","Health & Medical Sciences","Health Care Administration","Health Education","Laboratory","Medical & Dental Assistant","Medical Technology","Nursing","Nutrition","Pharmacy","Physical Therapy","Physician Assistant","Psychology & Mental Health","Public Health","Radiologic Science","Respiratory Therapy","Sports and Fitness"); var parent_6 = new Array("Baking & Pastry","Catering","Culinary Arts","Event Planning","Hospitality Management","Le Cordon Bleu","Travel & Tourism"); var parent_7 = new Array("Corrections","Court Reporting","Crime Scene & Forensics","Criminal Justice","Homeland Security","Investigation","Law","Paralegal & Legal Services","Public Safety","Security & Loss Prevention"); var parent_8 = new Array("Art History","Communications & Media","English","Ethnic Studies","General Studies","Language","Liberal Arts","Religious Studies","Writing"); var parent_9 = new Array("Aviation Science","Biology","Environmental Science","Mathematics & Statistics","Physics","Veterinary Science & Animal Care"); var parent_10 = new Array("Anthropology","Behavioral Science","Conflict Resolution & Mediation","Economics","Geography","History","International Relations","Political Science","Psychology & Behavior","Public Policy & Administration","Social Sciences","Social Work & Human Services","Sociology"); var parent_11 = new Array("Computer & Software Engineering","Computer Programming","Computer Science","Graphics & Mul