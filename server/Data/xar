Nurse (RN)","RN to BSN"); var parent_177 = new Array("Occupational Therapy"); var parent_179 = new Array("Counseling","Forensic Psychology"); var parent_186 = new Array("Chef Training"); var parent_188 = new Array("Hotel Management","Restaurant Management"); var parent_193 = new Array("Forensic Psychology","Forensic Science"); var parent_194 = new Array("Justice Administration","Juvenile Justice","Law Enforcement"); var parent_195 = new Array("Intelligence Studies"); var parent_212 = new Array("Christian Studies"); var parent_213 = new Array("Technical Writing"); var parent_215 = new Array("Aviation Mechanics & Technology"); var parent_216 = new Array("Biotechnology"); var parent_218 = new Array("Fire Science","Wildlife & Forestry"); var parent_231 = new Array("Behavioral Science","Counseling","School Counseling"); var parent_232 = new Array("Emergency Management","Public Policy"); var parent_234 = new Array("Social Work"); var parent_241 = new Array("Database Management","Information Assurance","Information Science","Information Systems Security"); var parent_244 = new Array("Computer Networking","Network Security"); var parent_245 = new Array("Network Security"); var parent_251 = new Array("Cisco Certification","Microsoft Certification","Oracle Certification","Sun Certification"); var parent_255 = new Array("Telecommunications"); function loadProgram() { var subject = document.searchbox.subject.value; var programs = document.searchbox.program; var concentrations = document.searchbox.concentration; programs.options.length = 0; if (subject == "All") { var newoption = document.createElement("OPTION"); newoption.text = "--- Select a Subject ---"; newoption.value = 0; programs.options.add(newoption, 0); programs.disabled = true; } else { var newprograms; switch(subject) { case "Art & Design": newprograms = parent_1; break; case "Business": newprograms = parent_2; break; case "Education": newprograms = parent_3; break; case "Engineering & Construction": newprograms = pa